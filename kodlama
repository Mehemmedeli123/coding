1)#include <iostream>
using namespace std;
#include <math.h>
int main()
{

    float x,y,z,t,w;
    cin>>x>>t>>z;
    y=z*cos(x-3.14/6)/(1/2+pow(sin(t),2));
    w=y+(z*z)/(3+z*z/5);
    cout<<y<<w;
}
